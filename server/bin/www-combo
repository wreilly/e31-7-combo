#!/usr/bin/env node

var appFromAppComboJs = require('../app-combo')

var port = 8089

var appName = 'wr-app-combo-some-times-rest-api'

appFromAppComboJs.listen(port, () => console.log(appName + ' listening on: ' + port + ' btw you should type \n nohup npm run start-combo & \n to run this server continuously (even after closing the terminal window). Cheers \n'))
